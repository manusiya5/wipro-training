<!DOCTYPE html>
<html>
<head>
  <title>My Courses</title>
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet">
</head>
<body class="container mt-5">
  <h2>My Courses</h2>

  <table class="table table-bordered">
    <tr>
      <th>Title</th>
      <th>Fee</th>
    </tr>
    <% courses.forEach(course => { %>
      <tr>
        <td><%= course.title %></td>
        <td><%= course.fee %></td>
      </tr>
    <% }) %>
  </table>
</body>
</html>
